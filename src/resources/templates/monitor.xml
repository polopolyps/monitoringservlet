<?xml version="1.0" encoding="UTF-8"?>

<!-- Used by the monitor servlet to have an object to test writing to. -->
<template-definition version="1.0" xmlns="http://www.polopoly.com/polopoly/cm/app/xml">
    <input-template name="example.Monitor">
        <editor>com.polopoly.cm.app.widget.OTopPolicyWidget</editor>
        <viewer>com.polopoly.cm.app.widget.OTopPolicyWidget</viewer>

        <field name="contentVersionLimiter" input-template="p.ContentVersionLimiter" label="Not displayed">
            <param name="maxVersions">3</param>
        </field>
    </input-template>
</template-definition>